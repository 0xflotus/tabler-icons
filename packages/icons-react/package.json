{
  "name": "@tabler/icons-react",
  "version": "1.0.0",
  "license": "MIT",
  "author": "codecalm",
  "description": "A set of free MIT-licensed high-quality SVG icons for you to use in your web projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler-icons.git",
    "directory": "packages/icons-react"
  },
  "main": "dist/cjs/tabler-icons-react.js",
  "main:umd": "dist/umd/tabler-icons-react.js",
  "module": "dist/esm/tabler-icons-react.js",
  "unpkg": "dist/umd/tabler-icons-react.min.js",
  "typings": "dist/tabler-icons-react.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn copy:license && yarn build:icons && yarn build:es && yarn build:bundles",
    "build:icons": "node build-icons.mjs",
    "build:es": "babel src -d dist/esm",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf ./src/icons/*.js"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.5.1 || ^17.0.0 || ^18.0.0"
  }
}
