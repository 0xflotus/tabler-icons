{
  "version": "2.0.0-alpha",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler-icons.git"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "sideEffects": false,
  "files": [
    "tags.json",
    "icons/*",
    "icons-png/*",
    "icons-react/index.d.ts",
    "icons-react/dist/*",
    "iconfont/**/*",
    "tabler-sprite.svg",
    "tabler-sprite-nostroke.svg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bundle exec jekyll serve --watch --livereload --trace --livereload_port 8888",
    "build": "pnpm optimize && pnpm update && pnpm build:jekyll && pnpm build:json && pnpm build:sprite && pnpm build:icons && pnpm build:react && pnpm build:preact && pnpm build:solidjs && pnpm build:svelte && pnpm build:vue && pnpm build:png && pnpm build:pdf  && pnpm update-readme",
    "update": "pnpm update:icons-version && pnpm update:icons-unicode",
    "update:icons-version": "node ./.build/update-icons-version.mjs",
    "update:icons-unicode": "node ./.build/update-icons-unicode.mjs",
    "optimize": "node ./.build/optimize.mjs",
    "import-tags": "node ./.build/import-tags.mjs",
    "import-categories": "node ./.build/import-categories.mjs",
    "_release": "git pull && release-it",
    "build:jekyll": "bundle exec jekyll build",
    "build:sprite": "echo 'ok' || node ./.build/build-sprite.mjs",
    "build:json": "node ./.build/build-icons-json.mjs",
    "build:icons": "pnpm run build --filter @tabler/icons",
    "build:react": "pnpm run build --filter @tabler/icons-react",
    "build:preact": "pnpm run build --filter=@tabler/icons-preact",
    "build:solidjs": "pnpm run build --filter=@tabler/icons-solidjs",
    "build:svelte": "pnpm run build --filter=@tabler/icons-svelte",
    "build:vue": "pnpm run build --filter=@tabler/icons-vue",
    "build:png": "pnpm run build --filter=@tabler/icons-pdf",
    "build:pdf": "pnpm --filter=@tabler/icons-pdf",
    "update-readme": "node ./.build/update-readme.mjs",
    "_build": "gulp build",
    "_build-iconfont": "gulp build-iconfont",
    "test": "pnpm workspaces run"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "7.11.6",
    "@babel/parser": "7.11.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "5.0.1",
    "@svgr/core": "5.4.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "4.2.3",
    "csv-parser": "^3.0.0",
    "fs-extra": "^10.1.0",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-iconfont": "11.0.1",
    "gulp-svgo": "^2.2.1",
    "gulp-zip": "5.1.0",
    "html-minifier": "^4.0.0",
    "lodash.template": "4.5.0",
    "minimist": "1.2.6",
    "node-sass": "8.0.0",
    "parse-svg-path": "^0.1.2",
    "prettier": "^2.8.1",
    "release-it": "14.12.5",
    "rollup": "2.78.1",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "svg-outline-stroke": "1.3.1",
    "svgo": "^2.8.0",
    "svgson": "^5.2.1"
  },
  "release-it": {
    "hooks": {
      "after:bump": "gulp build --latest-version ${latestVersion} --new-version ${version} --verbose",
      "after:release": "echo Successfully released ${name} v${latestVersion} to ${repo.repository}."
    },
    "git": {
      "addUntrackedFiles": true,
      "changelog": "gulp changelog --silent --latest-tag ${latestTag}",
      "tagName": "v${version}",
      "requireBranch": "master",
      "requireCommits": true
    },
    "github": {
      "release": true
    }
  },
  "packageManager": "pnpm@7.17.0"
}
