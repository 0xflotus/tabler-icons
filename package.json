{
  "version": "2.0.0-alpha",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler-icons.git"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "main": "./icons-react/dist/index.cjs.js",
  "exports": {
    ".": {
      "import": "./icons-react/dist/index.esm.js",
      "require": "./icons-react/dist/index.cjs.js"
    },
    "./iconfont/*": "./iconfont/*",
    "./*": [
      "./icons/*",
      "./icons-png/*"
    ]
  },
  "module": "./icons-react/dist/index.esm.js",
  "unpkg": "./icons-react/dist/index.umd.js",
  "umd:main": "./icons-react/dist/index.umd.js",
  "types": "./icons-react/index.d.ts",
  "sideEffects": false,
  "files": [
    "tags.json",
    "icons/*",
    "icons-png/*",
    "icons-react/index.d.ts",
    "icons-react/dist/*",
    "iconfont/**/*",
    "tabler-sprite.svg",
    "tabler-sprite-nostroke.svg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "bundle exec jekyll serve --watch --livereload --trace --livereload_port 8888",
    "build": "yarn optimize && yarn update && yarn build:jekyll && yarn build:icons-json && yarn build:sprite",
    "update": "yarn update:icons-version && yarn update:icons-unicode",
    "update:icons-version": "node ./.build/update-icons-version.mjs",
    "update:icons-unicode": "node ./.build/update-icons-unicode.mjs",
    "optimize": "node ./.build/optimize.mjs",
    "import-tags": "node ./.build/import-tags.mjs",
    "import-categories": "node ./.build/import-categories.mjs",
    "release": "git pull && release-it",
    "build:sprite": "node ./.build/build-sprite.mjs",
    "build:icons-json": "node ./.build/build-icons-json.mjs",
    "build:jekyll": "bundle exec jekyll build",
    "_build": "gulp build",
    "_build-iconfont": "gulp build-iconfont",
    "test": "yarn workspaces run"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "7.11.6",
    "@babel/parser": "7.11.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "5.0.1",
    "@svgr/core": "5.4.0",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "4.2.3",
    "csv-parser": "^3.0.0",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-iconfont": "11.0.1",
    "gulp-svgo": "^2.2.1",
    "gulp-zip": "5.1.0",
    "html-minifier": "^4.0.0",
    "lodash.template": "4.5.0",
    "minimist": "1.2.6",
    "node-sass": "8.0.0",
    "parse-svg-path": "^0.1.2",
    "prettier": "^2.7.1",
    "release-it": "14.12.5",
    "rollup": "2.78.1",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "svg-outline-stroke": "1.3.1",
    "svgo": "^2.8.0",
    "svgson": "^5.2.1"
  },
  "release-it": {
    "hooks": {
      "after:bump": "gulp build --latest-version ${latestVersion} --new-version ${version} --verbose",
      "after:release": "echo Successfully released ${name} v${latestVersion} to ${repo.repository}."
    },
    "git": {
      "addUntrackedFiles": true,
      "changelog": "gulp changelog --silent --latest-tag ${latestTag}",
      "tagName": "v${version}",
      "requireBranch": "master",
      "requireCommits": true
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "@vue/babel-plugin-jsx": "^1.1.1",
    "fs-extra": "^10.1.0"
  },
  "packageManager": "yarn@3.3.1"
}
